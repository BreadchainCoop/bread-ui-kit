{
  "name": "bread-ui-kit",
  "version": "1.0.0",
  "description": "A component library for implementing Bread Coop branding in JS/TS projects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style": "./dist/index.css",
    "./tailwind-preset": "./tailwind-preset.js",
    "./theme": "./theme.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "tailwind-preset.js",
    "theme.css",
    "fonts"
  ],
  "scripts": {
    "build": "tsup && npm run copy-files",
    "copy-files": "cp tailwind-preset.js dist/ && cp theme.css dist/ && cp -r fonts dist/",
    "dev": "tsup --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "ui",
    "components",
    "bread",
    "coop"
  ],
  "author": "Bread Coop",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@phosphor-icons/react": "^2.1.1",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-vite": "^9.1.8",
    "@storybook/react-webpack5": "^9.1.8",
    "@swc/core": "^1.3.100",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^9.1.8",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "storybook": "^9.1.8",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bread-coop/bread-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/bread-coop/bread-ui-kit/issues"
  },
  "homepage": "https://github.com/bread-coop/bread-ui-kit#readme"
}
